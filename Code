//  This program will caluate an equation
//  Created by Zeb on 8/18/20.
//

#include <stdio.h>

int main(void)
{
    int x, ans;
    
    printf("\nPlease input a number (integer) for x, to be caluated in the equation 3x^5 + 2x^4 - 5x^3 -x^2 + 7x -6: ");
    scanf("%d", &x);
    
    
    ans = (3 * x * x * x * x * x) + (2 *  x * x * x * x) + (-5 * x * x * x) + (-1 * x * x) + (7 * x) - 6;
    
    printf("\n\tThe answers to the equcation is: %d\n\n", ans);
    
    return 0;
}
